{
  "version": 2,
  "name": "esg-recommender",
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "frontend/build",
  "functions": {
    "api/app.py": {
      "runtime": "@vercel/python"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/build/$1"
    }
  ]
}
